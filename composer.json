{
    "name": "decodelabs/effigy",
    "description": "Universal CLI entry point",
    "type": "project",
    "keywords": [ "entry", "CLI" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",
        "composer-runtime-api": "^2.2",

        "decodelabs/archetype": "^0.4",
        "decodelabs/atlas": "^0.14",
        "decodelabs/chronicle": "^0.3",
        "decodelabs/clip": "^0.6",
        "decodelabs/coercion": "^0.3.3",
        "decodelabs/commandment": "^0.3",
        "decodelabs/dictum": "^0.7",
        "decodelabs/exceptional": "^0.6.1",
        "decodelabs/integra": "^0.3",
        "decodelabs/genesis": "^0.14",
        "decodelabs/hatch": "^0.1.4",
        "decodelabs/lucid": "^0.7",
        "decodelabs/monarch": "^0.2",
        "decodelabs/nuance": "^0.1.2",
        "decodelabs/systemic": "^0.12",
        "decodelabs/terminus": "^0.14",

        "ondram/ci-detector": "^4.2",
        "php-parallel-lint/php-parallel-lint": "^1.4",
        "symplify/easy-coding-standard": "^12.5.20"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Effigy\\": "src/Effigy/"
        },
        "classmap": [
            "src/Effigy.php"
        ]
    },
    "bin": [
        "bin/effigy"
    ],
    "extra": {
        "genesis": {
            "hub": "DecodeLabs\\Effigy\\Hub"
        },
        "branch-alias": {
            "dev-develop": "0.10.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "php-http/discovery": true,
            "decodelabs/genesis": true
        }
    },
    "repositories": {
    }
}
