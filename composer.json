{
    "name": "decodelabs/effigy",
    "description": "Universal CLI entry point",
    "type": "library",
    "keywords": [ "entry", "CLI" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",
        "composer-runtime-api": "^2.2",

        "decodelabs/archetype": "^0.3",
        "decodelabs/atlas": "^0.12",
        "decodelabs/chronicle": "^0.2",
        "decodelabs/clip": "^0.5",
        "decodelabs/coercion": "^0.3.0",
        "decodelabs/commandment": "^0.1.1",
        "decodelabs/dictum": "^0.6",
        "decodelabs/exceptional": "^0.5",
        "decodelabs/integra": "^0.2",
        "decodelabs/genesis": "^0.12.1",
        "decodelabs/lucid": "^0.5",
        "decodelabs/monarch": "^0.1.2",
        "decodelabs/systemic": "^0.11",
        "decodelabs/terminus": "^0.11.6",
        "decodelabs/veneer": "^0.12.3",

        "ondram/ci-detector": "^4.1",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "symplify/easy-coding-standard": "^12.5.6"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Effigy\\": "src/"
        }
    },
    "bin": [
        "bin/effigy"
    ],
    "extra": {
        "branch-alias": {
            "dev-develop": "0.6.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    }
}
