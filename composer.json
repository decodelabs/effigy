{
    "name": "decodelabs/effigy",
    "description": "Universal CLI entry point",
    "type": "library",
    "keywords": ["entry", "CLI"],
    "license": "MIT",
    "authors": [{
        "name": "Tom Wright",
        "email": "tom@inflatablecookie.com"
    }],
    "require": {
        "php": "^8.0",

        "decodelabs/archetype": "^0.2.9",
        "decodelabs/atlas": "^0.10.5",
        "decodelabs/dictum": "^0.4.3",
        "decodelabs/exceptional": "^0.4",
        "decodelabs/systemic": "^0.10.1",
        "decodelabs/terminus": "^0.9.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^1",
        "phpstan/extension-installer": "^1.0",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "symplify/easy-coding-standard": "^11",

        "decodelabs/phpstan-decodelabs": "^0.6",
        "decodelabs/glitch": "^0.18.5"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Effigy\\": "src/"
        }
    },
    "bin": [
        "bin/effigy"
    ],
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
