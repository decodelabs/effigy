{
    "name": "decodelabs/effigy",
    "description": "Universal CLI entry point",
    "type": "library",
    "keywords": [ "entry", "CLI" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.1",
        "composer-runtime-api": "^2.2",

        "decodelabs/archetype": "^0.2.9",
        "decodelabs/atlas": "^0.10.5|^0.11",
        "decodelabs/clip": "^0.3.2",
        "decodelabs/dictum": "^0.5",
        "decodelabs/exceptional": "^0.4",
        "decodelabs/integra": "^0.1.4",
        "decodelabs/genesis": "^0.7.1|^0.8",
        "decodelabs/lucid": "^0.4",
        "decodelabs/systemic": "^0.11",
        "decodelabs/terminus": "^0.10.1",
        "decodelabs/veneer": "^0.10.12",

        "phpstan/phpstan": "^1.8",
        "phpstan/extension-installer": "^1.2",
        "decodelabs/phpstan-decodelabs": "^0.6.1",
        "symplify/easy-coding-standard": "^11.1",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "ondram/ci-detector": "^4.1"
    },
    "require-dev": {
        "decodelabs/glitch": "^0.18.5"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Effigy\\": "src/"
        }
    },
    "bin": [
        "bin/effigy"
    ],
    "extra": {
        "branch-alias": {
            "dev-develop": "0.4.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "repositories": {
    }
}
